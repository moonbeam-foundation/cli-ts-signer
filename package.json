{
  "name": "cli-ts-signer",
  "version": "1.1.0",
  "description": "ts based signer for moonbeam",
  "main": "index.js",
  "repository": "git@github.com:PureStake/cli-ts-signer.git",
  "author": "Antoine Estienne <estienne.antoine@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/elliptic": "^6.4.12",
    "@types/node": "^15.6.1",
    "@types/tcp-port-used": "^1.0.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/prompts": "^2.0.13",
    "@types/yargs": "^17.0.1",
    "prettier": "^2.4.1",
    "chai": "^4.3.4",
    "mocha": "^9.0.3"
  },
  "scripts": {
    "cli": "node_modules/.bin/ts-node src/index.ts",
    "cli-binary-macos": "./moonbeam-signer-macos",
    "test": "mocha -r ts-node/register --exit 'test/**.spec.ts'",
    "test-local": "LOCAL_BUILD=true mocha -r ts-node/register --exit 'test/**.spec.ts'",
    "lint": "node_modules/.bin/prettier --check .",
    "lint:fix": "node_modules/.bin/prettier -w .",
    "build-binary": "npx @vercel/ncc build src/index.ts -o dist ; node_modules/.bin/pkg -t node14-linux-x64,node14-macos-x64,node14-win-x64 -o moonbeam-signer dist/index.js"
  },
  "dependencies": {
    "@polkadot/api": "^7.3.1",
    "@polkadot/api-derive": "^7.3.1",
    "@polkadot/keyring": "^8.3.1",
    "@polkadot/rpc-augment": "^7.3.1",
    "@polkadot/rpc-core": "^7.3.1",
    "@polkadot/types": "^7.3.1",
    "@polkadot/util": "^8.3.1",
    "@polkadot/wasm-crypto": "^4.5.1",
    "chalk": "^4.1.2",
    "child_process": "^1.0.2",
    "elliptic": "^6.5.4",
    "moonbeam-types-bundle": "^2.0.2",
    "ncc": "^0.3.6",
    "pkg": "^5.3.0",
    "prompts": "^2.4.1",
    "tcp-port-used": "^1.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "web3": "^1.6.1",
    "yargs": "^17.0.1"
  }
}
